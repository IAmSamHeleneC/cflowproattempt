//CFlow
//HelloWorld Test
public class Cflow{
    public static void main(String[] args) throws Exception {
        System.out.println("Hello, World!");  }
    
    
        public static void printNumbers()
  { // TODO: print valid numeric input
    char myChar= 79; 
    System.out.println (myChar);
  }

  public static void printLowerCase()
  {// TODO: print valid lowercase alphabetic input
    char myLow= ('a'); 
    System.out.println (myLow);
   }

  public static void printUpperCase()
  { // TODO: print valid uppercase alphabetic input
    char myUp= ('H');
    System.out.println(myUp);

    //find range of values, then use for loop to print valid values
    //7 bits
    //2^7 = 128
    // 0-127
    for(boolean i=0 <=127;;) {
      System.out.printf ("%d : %c",i,i);
    }
    }
}
//ASciiChars
public class AsciiChars {
    public static void main(String[] args) {
        System.out.print("The numbers are: ");
        printNumbers();
        System.out.print("The lower case letters are: ");
        printLowerCase();
        System.out.print("The upper case letters are: ");
        printUpperCase();
    }

    public static void printNumbers() {
        // print valid numeric input
        // 48-57
        for (int i = 48; i < 57; i++) {
            System.out.print((char)i);
        }
        System.out.print((char)57 + "\n");
    }
    public static void printLowerCase() {
        // print valid lowercase alphabetic input
        // 97 - 122
        for (int i = 97; i <= 122; i ++) {
            System.out.print((char)i);
        }
        System.out.println();
    }
    public static void printUpperCase() {
        // print valid uppercase alphabetic input
        // 65 - 90
        for (int i = 65; i < 90; i++) {
            System.out.print((char)i + " ");
        }
        System.out.print((char)90 + "\n");
    }
}
//
